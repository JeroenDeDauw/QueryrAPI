    "parameters": {
        "per_page": {
            "name": "per_page",
            "in": "query",
            "description": "Maximum number of records to return",
            "required": false,
            "type": "integer",
            "format": "int32",
            "minimum": 1,
            "maximum": 1000,
            "default": 100
        },
        "page": {
            "name": "page",
            "in": "query",
            "description": "Pagination parameter",
            "required": false,
            "type": "integer",
            "format": "int32",
            "minimum": 1,
            "maximum": 100,
            "default": 1
        },
        "path_item_id": {
            "in": "path",
            "name": "item_id",
            "description": "ID of the item. Example: Q64",
            "required": true,
            "type": "string"
        },
        "path_property_id": {
            "in": "path",
            "name": "property_id",
            "description": "ID of the property. Example: P31",
            "required": true,
            "type": "string"
        }
    },
    "definitions": {
        "item_list_element": {
            "type": "object",
            "required": [ "id", "updated_at", "url", "wikidata_url" ],
            "properties": {
                "id": {
                    "type": "string",
                    "example": "Q60"
                },
                "label": {
                    "type": "string",
                    "example": "New York City"
                },
                "updated_at": {
                    "type": "string",
                    "example": "2015-11-24T23:19:46Z"
                },
                "url": {
                    "type": "string",
                    "example": "http://queryr.wmflabs.org/items/Q60"
                },
                "wikidata_url": {
                    "type": "string",
                    "example": "https://www.wikidata.org/entity/Q60"
                },
                "wikipedia_url": {
                    "type": "string",
                    "example": "https://www.wikidata.org/wiki/Special:GoToLinkedPage/enwiki/Q60"
                }
            }
        },
        "property_list_element": {
            "type": "object",
            "required": [ "id", "type", "url", "wikidata_url" ],
            "properties": {
                "id": {
                    "type": "string",
                    "example": "P281"
                },
                "type": {
                    "type": "string",
                    "example": "string"
                },
                "url": {
                    "type": "string",
                    "example": "http://queryr.wmflabs.org/properties/P281"
                },
                "wikidata_url": {
                    "type": "string",
                    "example": "https://www.wikidata.org/entity/P281"
                }
            }
        },
        "item_type_list_element": {
            "type": "object",
            "required": [ "label", "id", "url", "wikidata_url" ],
            "properties": {
                "label": {
                    "type": "string",
                    "example": "cat"
                },
                "id": {
                    "type": "string",
                    "example": "Q146"
                },
                "url": {
                    "type": "string",
                    "example": "http://queryr.wmflabs.org/items/Q146"
                },
                "wikidata_url": {
                    "type": "string",
                    "example": "https://www.wikidata.org/entity/Q146"
                }
            }
        },
        "simple_statement": {
            "type": "object",
            "required": [ "value", "type" ],
            "properties": {
                "value": {
                    "type": "object",
                    "example": "Los Angeles"
                },
                "type": {
                    "type": "string",
                    "example": "string"
                },
                "values": {
                    "type": "array",
                    "items": {
                        "type": "object"
                    },
                    "minItems": 2,
                    "example": [
                        "Los Angeles",
                        "Madrid",
                        "Istanbul"
                    ]
                }
            }
        },
        "simple_statement_map": {
            "type": "object",
            "additionalProperties": {
                "$ref": "#/definitions/simple_statement"
            }
        }
    },
