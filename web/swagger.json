{
    "swagger": "2.0",
    "info": {
        "version": "0.0.1",
        "title": "QueryR API"
    },
    "host": "queryr.wmflabs.org",
    "basePath": "/api",
    "schemes": [
        "http"
    ],
    "parameters": {
        "per_page": {
            "name": "per_page",
            "in": "query",
            "description": "Maximum number of records to return",
            "required": false,
            "type": "integer",
            "format": "int32",
            "minimum": 1,
            "maximum": 1000,
            "default": 100
        },
        "page": {
            "name": "page",
            "in": "query",
            "description": "Pagination parameter",
            "required": false,
            "type": "integer",
            "format": "int32",
            "minimum": 1,
            "maximum": 100,
            "default": 1
        },
        "path_item_id": {
            "in": "path",
            "name": "item_id",
            "description": "ID of the item. Example: Q64",
            "required": true,
            "type": "string"
        },
        "path_property_id": {
            "in": "path",
            "name": "property_id",
            "description": "ID of the property. Example: P31",
            "required": true,
            "type": "string"
        }
    },
    "paths": {
        "/": {
            "get": {
                "tags": [ "root" ],
                "description": "Returns a hash with urls of noteworthy endpoints.",
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/items": {
            "get": {
                "tags": [ "items" ],
                "description": "Lists item summaries. Each summary holds a link to the full item.",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "$ref": "#/parameters/per_page"
                    },
                    {
                        "$ref": "#/parameters/page"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/items/{item_id}": {
            "get": {
                "tags": [ "items" ],
                "description": "Returns a single item.",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "$ref": "#/parameters/path_item_id"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "400": {
                        "description": "Invalid ID supplied"
                    },
                    "404": {
                        "description": "Item not found"
                    }
                }
            }
        },
